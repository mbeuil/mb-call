{
  "name": "mb-call",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "is-ci test:coverage test:watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci test:e2e:run test:e2e:dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start 3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev 3000 cy:open",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json}'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "prepare": "husky install",
    "commit": "git-cz",
    "validate": "npm-run-all --parallel check-format lint build "
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "next": "11.1.0",
    "next-localization": "^0.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^27.0.1",
    "@types/react": "17.0.18",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "autoprefixer": "^10.3.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "commitizen": "^4.2.4",
    "cypress": "^8.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-next": "0.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-testing-library": "^4.11.0",
    "husky": "^7.0.1",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.13.1",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5"
  }
}
