{
  "plugins": ["cypress"],
  "extends": ["plugin:cypress/recommended"],
  "env": {
    "cypress/globals": true
  },
  "overrides": [
    {
      "files": ["**/*.ts"],
      "rules": {
        "jest/expect-expect": "off",
        "testing-library/await-async-query": "off",
        "testing-library/prefer-screen-queries": "off"
      }
    }
  ]
}
